{
  "name": "boilerplate-apps-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "prepare": "husky install",
    "test": "jest",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema-temp.gql --tagName=gql --outputFlat src/graphql/generated",
    "generate": "npx plop --plopfile generators/plopfile.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@styled-icons/boxicons-regular": "^10.47.0",
    "@styled-icons/boxicons-solid": "^10.47.0",
    "@styled-icons/fa-brands": "^10.47.0",
    "@types/react-slick": "^0.23.13",
    "graphql": "^16.9.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "14.2.3",
    "polished": "^4.3.1",
    "react": "^18",
    "react-dom": "^18",
    "react-slick": "^0.30.2",
    "styled-components": "^6.1.11",
    "styled-icons": "^10.47.1",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@chromatic-com/storybook": "^1.5.0",
    "@eslint/js": "^9.4.0",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/addon-links": "^8.1.6",
    "@storybook/addon-onboarding": "^8.1.6",
    "@storybook/blocks": "^8.1.6",
    "@storybook/nextjs": "^8.1.6",
    "@storybook/react": "^8.1.6",
    "@storybook/test": "^8.1.6",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/styled-components": "^5.1.34",
    "apollo": "^2.34.0",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "globals": "^15.4.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "lint-staged": "^15.2.5",
    "plop": "^4.0.1",
    "prettier": "3.3.1",
    "storybook": "^8.1.6",
    "typescript": "^5",
    "typescript-eslint": "^7.12.0"
  }
}
